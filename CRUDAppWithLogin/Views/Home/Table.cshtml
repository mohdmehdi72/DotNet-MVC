@model List<CRUDAppWithLogin.Models.MyModel>
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Tables</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        DataTables Advanced Tables
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>Name</th>
                                        <th>Age</th>
                                        <th>Salary</th>
                                        <th>City</th>
                                        <th>Edit</th>
                                        <th>Delete</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    @foreach (var item in Model)
                                    {
                                        <tr class="odd gradeX">
                                            <td>@item.id</td>
                                            <td>@item.ename</td>
                                            <td>@item.age</td>
                                            <td>@item.salay</td>
                                            <td>@item.city</td>
                                            <td><a href="@Url.Action("Edit","Home", new { item.id})"><i class="fa fa-edit"></i></a></td>
                                            <td><a href="@Url.Action("Delete","Home", new { item.id})"><i class="fa fa-trash"></i></a></td>
                                        </tr>

                                    }

                                </tbody>
                            </table>
                        </div>
                        <!-- /.table-responsive -->

                    </div>

                    <!-- /.panel-body -->
                    <div class="panel-heading">
                        Users Table
                    </div>
                    <!-- /.panel-heading -->
                   
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    <!-- /.container-fluid -->
</div>
</div>
